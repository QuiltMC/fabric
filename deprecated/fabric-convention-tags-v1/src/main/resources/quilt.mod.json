{
	"schema_version": 1,
	"quilt_loader": {
		"group": "org.quiltmc.quilted-fabric-api",
		"id": "quilted_fabric_convention_tags_v1",
		"version": "${version}",
		"metadata": {
			"name": "Quilted Fabric Convention Tags",
			"description": "Contains common tags used by mods for vanilla things.",
			"contributors": {
				"The Quilt Project": "Owner",
				"FabricMC": "Upstream Owner"
			},
			"contact": {
				"homepage": "https://quiltmc.org",
				"issues": "https://github.com/QuiltMC/quilted-fabric-api/issues",
				"sources": "https://github.com/QuiltMC/quilted-fabric-api"
			},
			"license": "Apache-2.0",
			"icon": "assets/fabric-convention-tags-v1/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"load_type": "always",
		"depends": [
			{
				"id": "quilt_loader",
				"versions": ">=0.25.0"
			},
			{
				"id": "minecraft",
				"versions": ">=1.20.5- <1.20.7-"
			}
		],
		"provides": [
			{
				"id": "fabric-convention-tags-v1",
				"version": "${upstream_version}"
			}
		],
		"entrypoints": {
			"main": [
				"net.fabricmc.fabric.impl.tag.convention.ConventionLogWarnings"
			]
		}
	},
	"minecraft": {
		"environment": "*"
	},
	"modmenu": {
		"badges": [
			"library"
		],
		"parent": {
			"id": "quilted_fabric_api"
		}
	},
	"fabric-api:module-lifestyle": "stable"
}