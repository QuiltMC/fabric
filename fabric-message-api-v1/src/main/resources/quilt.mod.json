{
	"schema_version": 1,
	"quilt_loader": {
		"group": "org.quiltmc.quilted-fabric-api",
		"id": "quilted_fabric_message_api_v1",
		"version": "${version}",
		"metadata": {
			"name": "Quilted Fabric Message API (v1)",
			"description": "Adds message-related hooks.",
			"contributors": {
				"The Quilt Project": "Owner",
				"FabricMC": "Upstream Owner"
			},
			"contact": {
				"homepage": "https://quiltmc.org",
				"issues": "https://github.com/QuiltMC/quilted-fabric-api/issues",
				"sources": "https://github.com/QuiltMC/quilted-fabric-api"
			},
			"license": "Apache-2.0",
			"icon": "assets/fabric-message-api-v1/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"load_type": "always",
		"depends": [
			{
				"id": "quilt_loader",
				"versions": ">=0.26.3"
			},
			{
				"id": "minecraft",
				"versions": ">=1.21- <1.21.2-"
			}
		],
		"provides": [
			{
				"id": "fabric-message-api-v1",
				"version": "${upstream_version}"
			}
		]
	},
	"mixin": [
		"fabric-message-api-v1.mixins.json",
		{
			"config": "fabric-message-api-v1.client.mixins.json",
			"environment": "client"
		}
	],
	"minecraft": {
		"environment": "*"
	},
	"modmenu": {
		"badges": [
			"library"
		],
		"parent": {
			"id": "quilted_fabric_api"
		}
	},
	"fabric-api:module-lifestyle": "stable"
}