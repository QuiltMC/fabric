			{
				"id": "fabric-renderer-indigo",
				"version": "${upstream_version}"
			}
		]
	},
  "minecraft": {
    "environment": "client"
  },
	"mixin": "quilted_fabric_renderer_indigo.mixins.json",
	"fabric-api:module-lifecycle": "stable",
	"modmenu": {
		"badges": [
			"library"
		],
		"parent": "quilted_fabric_api"
	}
