{
	"schema_version": 1,
	"quilt_loader": {
		"group": "org.quiltmc",
		"id": "quilted_fabric_api",
		"version": "${version}",
		"metadata": {
			"name": "Quilted Fabric API",
			"description": "A reimplementation of Fabric API on top of Quilt Standard Libraries.",
			"contributors": {
				"Owner": "QuiltMC: QSL Team",
				"Original Owner": "FabricMC"
			},
			"contact": {
				"homepage": "https://quiltmc.org",
				"issues": "https://github.com/QuiltMC/quilted-fabric-api/issues",
				"sources": "https://github.com/QuiltMC/quilted-fabric-api"
			},
			"license": "Apache-2.0",
			"icon": "assets/quilted_fabric_api/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"depends": [
			"quilt_loader",
			{
				"id": "java",
				"versions": ">=17"
			},
			{
					"id": "minecraft",
					"versions": "~1.18.2-alpha.22.5.a"
			}
		],
		"provides": [
			{
				"id": "fabric",
				"version": "${upstream_version}"
			}
		]
	},
	"modmenu": {
		"badges": [
			"library"
		]
	}
}
