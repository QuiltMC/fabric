{
	"schema_version": 1,
	"quilt_loader": {
		"group": "org.quiltmc.quilted-fabric-api",
		"id": "quilted_fabric_api",
		"version": "${version}",
		"metadata": {
			"name": "Quilted Fabric API",
			"description": "Core API module providing key hooks and intercompatibility features.",
			"contributors": {
				"The Quilt Project": "Owner",
				"FabricMC": "Upstream Owner"
			},
			"contact": {
				"homepage": "https://quiltmc.org",
				"issues": "https://github.com/QuiltMC/quilted-fabric-api/issues",
				"sources": "https://github.com/QuiltMC/quilted-fabric-api"
			},
			"license": "Apache-2.0",
			"icon": "assets/fabric/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"load_type": "always",
		"depends": [
			{
				"id": "quilt_loader",
				"versions": ">=0.25.0"
			},
			{
				"id": "minecraft",
				"versions": ">=1.21- <1.21.2-"
			}
		],
		"provides": [
			{
				"id": "fabric-api",
				"version": "${upstream_version}"
			},
			{
				"id": "fabric",
				"version": "${upstream_version}"
			}
		]
	},
	"minecraft": {
		"environment": "*"
	},
	"modmenu": {
		"badges": [
			"library"
		],
		"parent": {
			"id": "quilted_fabric_api"
		}
	}
}